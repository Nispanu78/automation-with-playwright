{
  "name": "automation-exercise-tests",
  "version": "1.0.0",
  "description": "E2E Test Automation Suite for automationexercise.com",
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
